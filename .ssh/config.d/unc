Host login.cs.unc.edu
  HostName=login.cs.unc.edu
  User=pganguli
  PreferredAuthentications=publickey
  IdentityFile=~/.ssh/ssh_ed25519
  Compression=yes

Host longleaf.unc.edu
  HostName=longleaf.unc.edu
  User=pganguli
  PreferredAuthentications=publickey
  IdentityFile=~/.ssh/ssh_ed25519
  Compression=yes

Host chaynick.cs.unc.edu
  HostName=chaynick.cs.unc.edu
  User=pganguli
  PreferredAuthentications=publickey,password
  IdentityFile=~/.ssh/ssh_ed25519
  RequestTTY yes
  RemoteCommand tmux new -A -s ssh_pganguli
  Compression=no

Host chaynick.login.cs.unc.edu
  ProxyJump=login.cs.unc.edu
  HostName=chaynick.cs.unc.edu
  User=pganguli
  PreferredAuthentications=publickey,password
  IdentityFile=~/.ssh/ssh_ed25519
  Compression=yes

